
-#if(@menu_access[:download_proceedings])

-#ddownload_proceedings= @data.inspect

-if @data
 #download_proceedings
  %table{align: :center, border: 1, cellpadding: 10}
   %thead
    %tr
     -%w[lang_en lang_ru].each do |lang|
      %th= _(lang)
    -%w[link_toc link_archive].each do |lt|
     %tr
      -rec = @data[lt]
      -if rec
       -%w[en ru].each do |lang|
        %td
         -lnk = rec[lang]
         -if !rec[lang].blank?
          %a{href: rec[lang]}= _(lt)
     

